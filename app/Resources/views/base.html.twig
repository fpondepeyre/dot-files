<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8"/>
    <title>{% block title %}Welcome!{% endblock %}</title>
    {% block stylesheets %}
    <link href="{{ asset('bundles/dytwebsite/css/bootstrap.min.css') }}" rel="stylesheet">
    <link href="{{ asset('bundles/dytwebsite/css/bootstrap-responsive.min.css') }}" rel="stylesheet">
    <link href="{{ asset('bundles/dytwebsite/css/main.css') }}" rel="stylesheet">
    {% endblock %}
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}"/>
</head>
<body>
    {% block body %}Welcome!{% endblock %}
    {% block javascripts %}
        <script src="//ajax.googleapis.com/ajax/libs/jquery/1.8.2/jquery.min.js"></script>
        <script>window.jQuery || document.write('<script src="{{ asset('bundles/dytwebsite/js/jquery-1.8.2.min.js') }}"><\/script>')</script>
        <script src="{{ asset('bundles/dytwebsite/js/bootstrap.min.js') }}"></script>
    {% endblock %}
</body>
</html>
